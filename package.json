{
  "name": "markdown-editor",
  "version": "1.0.0",
  "description": "a markdown editor/previewer written in node.js/express and react",
  "main": "index.js",
  "author": "fafafariba",
  "license": "MIT",
  "scripts": {
    "start:api": "cd api && nodemon server.js",
    "start:client": "cd client && yarn start",
    "start:dev": "concurrently  --kill-others-on-fail \"yarn start:api\" \"yarn start:client\""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "nodemon": "^1.18.10"
  }
}
